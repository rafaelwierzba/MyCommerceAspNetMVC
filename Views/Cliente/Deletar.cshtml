
@{
    <h2>Deletar Cliente</h2>
    <h3>Deseja realmente Deletar o Cliente Selecionado? </h3>
    <button type="button" class="btn btn-success" onclick="Sim()">Sim</button>
    <button type="button" class="btn btn-danger" onclick="Nao()">Não</button>

    <script>
        function Sim(){
            window.location.href = `../../Cliente/DeletarCliente/@ViewData["idDelete"]`;
        }

        function Nao(){
            window.location.href = '../../Cliente/Index';
        }
    </script>
}
